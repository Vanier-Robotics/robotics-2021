nav#navbar.collapsed()
	.bubble.bubble-top(onclick="toggle()")
	ul#navlist
		.navheader
			a
				img.navimg(src="/assets/images/logos/crclogo.png")
			a
				img.navimg(src="/assets/images/logos/vanier roboticslogo.png")
			a 
				span.navtoggle=text.navbar.header.toggle[lang]
		+navlink("/", text.navbar.links.map[lang], "")
		+navlink("/roster/", text.navbar.links.roster[lang], "Militia.png")
		+navlink("/robot/", text.navbar.links.robot[lang], "Warfront.png")
		+navlink("/archives/", text.navbar.links.archives[lang], "Archives.png")
		+navlink("/video/", text.navbar.links.video[lang], "Ampitheater.svg")
	.bubble.bubble-bottom(onclick="toggle()")